{
  "name": "@solana-hack-nft/backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register ./src/index",
    "typeorm": "ts-node -r tsconfig-paths/register --files ./node_modules/typeorm/cli.js",
    "db:migration:generate": "yarn typeorm migration:generate",
    "db:migration:run": "yarn typeorm migration:run",
    "db:migration:revert": "yarn typeorm migration:revert",
    "create-metadata": "ts-node -r tsconfig-paths/register ./scripts/create-metadata"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.12.0",
    "typeorm": "^0.2.44"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "ts-node-dev": "^1.1.8"
  }
}
